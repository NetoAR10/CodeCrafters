<%- include('includes/sidebar.ejs'), {
    correo: correo,
    permisos: permisos,
    rol: rol,
    nombre: nombre,
    rolUser: rolUser,
} %>
<!DOCTYPE html>
<html>
    <head>
        <title>Página de Inicio</title>
    </head>
    <body>
        <div class="page_content">
            <div class="text">
                <div class="title_usuarios">
                   <h1>Usuarios</h1> 
                </div>
                <input id="buscar" class="buscar" type="text" placeholder="Buscar..."/><br><br>
                <table id="USUARIOS">
                    <tr>
                        <th>Nombre</th>
                        <th>Matrícula</th>
                        <th>Correo electrónico</th>
                        <th>Beca</th>
                        <th>Rol</th>
                    </tr>
                    <% for (let user of usuariosDB) {%>
                        <tr>
                          <td><%= user.Nombre %></td>
                          <td><%= user.Matricula %></td>
                          <td><%= user.Correo_electronico %></td>
                          <td><%= user.Beca_actual %></td>
                          <td><%= user.Tipo_Rol %> <button class="adminrole" onclick="alert('Cambiar rol')">Cambiar</button> </td>
                        </tr>
                    <%}%>
                    
                    
                  </table>
            </div>
        </div>
        
    </body>
</html>